   "source": [
    "import sys\n",
    "k = int(sys.stdin.readline())\n",
    "\n",
    "nums = [\n",
    "    int(sys.stdin.readline())\n",
    "    for _ in range(k)\n",
    "]\n",
    "\n",
    "stack = []\n",
    "for num in nums:\n",
    "    if num == 0:\n",
    "        stack.pop()\n",
    "    else:\n",
    "        stack.append(num)\n",
    "\n",
    "print(sum(stack))"
   ]
  }
