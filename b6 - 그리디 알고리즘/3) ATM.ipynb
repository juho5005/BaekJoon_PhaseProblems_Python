   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "5\n",
      "3 1 4 3 2\n",
      "32\n"
     ]
    }
   ],
   "source": [
    "n = int(input()) # 사람의 수\n",
    "\n",
    "times = list(map(int, input().split()))\n",
    "times.sort()\n",
    "\n",
    "result = 0\n",
    "interval = 0\n",
    "for i in times : \n",
    "    interval += i \n",
    "    result += interval\n",
    "print(result)\n",
    "    "
