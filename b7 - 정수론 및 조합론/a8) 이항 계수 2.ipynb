   "source": [
    "# 이항 계수 1\n",
    "# 자연수 N과 정수 K가 주어질 때 이항 계수 (N, K)를 구하시오.\n",
    "\n",
    "import sys\n",
    "n, k = tuple(map(int, sys.stdin.readline().split()))\n",
    "\n",
    "a = 1\n",
    "x = n\n",
    "for _ in range(k):\n",
    "    a *= x\n",
    "    x -= 1\n",
    "\n",
    "b = 1\n",
    "y = k\n",
    "for _ in range(k):\n",
    "    b *= y\n",
    "    y -= 1\n",
    "\n",
    "print((a//b) % 10007)"
   ]
  }
 ],
