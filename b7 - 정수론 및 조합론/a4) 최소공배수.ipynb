   "source": [
    "# 최소공배수\n",
    "import sys\n",
    "t = int(sys.stdin.readline())\n",
    "\n",
    "def gcd(a, b):\n",
    "    while b > 0:\n",
    "        a, b = b, a % b\n",
    "    return a\n",
    "\n",
    "for _ in range(t):\n",
    "    a, b = tuple(map(int, sys.stdin.readline().split()))\n",
    "    print(a*b//gcd(a, b))"
   ]
  }
