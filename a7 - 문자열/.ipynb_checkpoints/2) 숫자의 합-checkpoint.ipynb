   "source": [
    "위의 각 자리의 수를 더하는 방식을 더 간단히 하는 방식"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "id": "7ab492a7",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "5\n",
      "54321\n",
      "15\n"
     ]
    }
   ],
   "source": [
    "n = int(input())\n",
    "\n",
    "nums = input()\n",
