   ],
   "source": [
    "n = int(input()) # 숫자의 개수\n",
    "\n",
    "nums = input()\n",
    "\n",
    "# 각 자리의 수를 더하는 방식\n",
    "\n",
    "result = 0\n",
    "for i in nums :\n",
    "    result += int(i)\n",
    "print(result)"
   ]
